{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/terms",
      "dest": "/app.py"
    },
    {
      "src": "/privacy",
      "dest": "/app.py"
    },
    {
      "src": "/verify",
      "dest": "/app.py"
    },
    {
      "src": "/bypass-verification",
      "dest": "/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
